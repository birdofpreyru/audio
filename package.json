{
	"name": "@dr.pogodin/audio",
	"version": "1.0.0",
	"description": "Web audio utils",
	"main": "./dist/bundle.js",
	"scripts": {
		"start": "webpack --mode=development --watch --config=./webpack.config.js",
		"watch": "webpack --mode=development --watch --config=./webpack.config.js",
		"prebuild": "rimraf ./dist",
		"build": "npm run prebuild && webpack --mode=production --config=./webpack.config.js",
		"prepublish": "npm run build",
		"version": "npm run build",
		"postversion": "git push && git push --follow-tags",
		"lint": "eslint ./src",
		"test": "npm run lint"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/birdofpreyru/audio.git"
	},
	"keywords": [
		"npm",
		"node",
		"audio",
		"analyser",
		"pitch",
		"sound",
		"javascript"
	],
	"license": "MIT",
	"files": [
		"dist"
	],
	"bugs": {
		"url": "https://github.com/birdofpreyru/audio/issues"
	},
	"homepage": "https://github.com/birdofpreyru/audio#readme",
	"devDependencies": {
		"babel-core": "^6.26.3",
		"babel-loader": "^7.1.5",
		"babel-plugin-transform-object-rest-spread": "^6.26.0",
		"babel-preset-env": "^1.7.0",
		"eslint": "^4.19.1",
		"eslint-config-airbnb-base": "^13.0.0",
		"eslint-plugin-import": "^2.13.0",
		"rimraf": "^2.6.2",
		"uglifyjs-webpack-plugin": "^1.3.0",
		"webpack": "^4.17.1",
		"webpack-cli": "^3.1.0"
	},
	"dependencies": {}
}
